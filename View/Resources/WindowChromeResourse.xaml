<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BackgroundChrome">#88FFAA</Color>
    <SolidColorBrush x:Key="BackgroundChromeBrush" Color="{StaticResource BackgroundChrome}"/>
    <Color x:Key="ForegroundChrome">#550088</Color>
    <SolidColorBrush x:Key="ForegroundChromeBrush" Color="{StaticResource ForegroundChrome}"/>
</ResourceDictionary>